{
    "name": "gquemener/unused-argument",
    "description": "PhpStan extension to detect unused function argument",
    "type": "phpstan-extension",
    "require": {
        "phpstan/phpstan": "^1.10"
    },
    "require-dev": {
        "behat/behat": "^3.13",
        "symfony/process": "^6.3",
        "phpunit/phpunit": "^10.2",
        "phpstan/extension-installer": "^1.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gquemener\\UnusedArgument\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gquemener\\UnusedArgument\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gildas Quéméner",
            "email": "gildas.quemener@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/extension.neon"
            ]
        }
    }
}
